!
!
hostname spine-2
!
!
ip routing
!
spanning-tree mode rapid-pvst
!
!vlan 104
!
!vlan 105
!
!vlan 106
!
!
router ospf 100
!
  redistribute connected
  !
  network 10.250.0.0/23 area 0
  !
  log-adjacency-changes detail
!
interface eth1
  channel-group 1 mode on
!  switchport mode trunk
!  switchport trunk vlan allowed 1,104
!  ip address 10.250.0.13/30
!
interface eth2
  channel-group 2 mode on
!  switchport mode trunk
!  switchport trunk vlan allowed 1,105
!  ip address 10.250.0.17/30
!
interface eth3
  channel-group 3 mode on
!  switchport mode trunk
!  switchport trunk vlan allowed 1,106
!  ip address 10.250.0.21/30
!
!
interface po1
  no switchport 
  ip address 10.250.0.13/30
!
interface po2
  no switchport
  ip address 10.250.0.17/30
!
interface po3
  no switchport
  ip address 10.250.0.21/30
!
!interface vlan 104
!  ip address 10.250.0.13/30
!
!interface vlan 105
!  ip address 10.250.0.17/30
!
!interface vlan 106
!  ip address 10.250.0.21/30
!
!
interface lo0
  ip address 10.250.1.2/32